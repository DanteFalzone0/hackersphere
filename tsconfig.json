{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es2021",
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "rootDir": "src",
    "diagnostics": true,
    "extendedDiagnostics": true,
    "listFiles": true,
    "traceResolution": true
  },
  "include": ["src"]
}
